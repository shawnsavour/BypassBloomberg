{
   "browser_action": {
      "default_icon": {
         "16": "icons/icon-16x16.png"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": ["content.js"],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Bypass bloomberg by shawn",
   "icons": {
      "128": "icons/icon-128x128.png",
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png"
   },
   "manifest_version": 2,
   "name": "Bypass Bloomberg",
   "permissions": [ "webRequest", "webRequestBlocking", "<all_urls>", "tabs", "http://*/*", "https://*/*"],
   "version": "2.4.0"
}
